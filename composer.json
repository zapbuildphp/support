{
    "name": "zvestaphp/support",
    "description": "Rinvex common support helpers, contracts, and traits required by various Rinvex packages. Validator functionality, and basic controller included out-of-the-box.",
    "keywords": [
        "rinvex",
        "laravel",
        "contract",
        "validator",
        "timezones",
        "mimetype",
        "support",
        "helper",
        "trait"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com/marketplace/rinvex-support/",
    "support": {
        "email": "help@zvesta.com",
        "issues": "https://github.com/zvestaphp/support/issues",
        "source": "https://github.com/zvestaphp/support",
        "docs": "https://github.com/zvestaphp/support/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Zvesta LLC",
            "homepage": "https://zvesta.com",
            "email": "help@zvesta.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/zvestaphp/support/contributors"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "illuminate/bus": "5.5.0",
        "illuminate/http": "5.5.0",
        "illuminate/support": "5.5.0",
        "illuminate/contracts": "5.5.0",
        "illuminate/validation": "5.5.0",
        "illuminate/routing": "5.5.0",
        "illuminate/auth": "5.5.0"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Rinvex\\Support\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
